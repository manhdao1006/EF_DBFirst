@model List<EF_DBFirst.Models.Khoa>
@{
    ViewBag.Title = "Khoas";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Khoas</h2>
<table class="table">
    <tr>
        <th>Mã khoa</th>
        <th>Tên khoa</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
    }
</table>

